<!DOCTYPE html>
<html data-theme="winter">
  <head>
    <title>Higher Eduction Access Prediction System</title>
    <link rel="stylesheet" href="/stylesheets/style.css" />
    <link rel="stylesheet" href="/stylesheets/aos.css" />
    <link rel="stylesheet" href="/stylesheets/swiper-bundle.min.css" />
    <script src="/javascripts/swiper-bundle.min.js"></script>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  </head>
  <body>
    <% include ./partial/navbar.ejs %>
    <div class="carousel w-full h-[50vh] lg:h-[80vh]">
      <div id="slide1" class="carousel-item relative w-full">
        <img src="/images/slider-1-2.jpg" class="w-full object-cover" />
        <div
          class="absolute top-1/2 left-5 lg:left-16 -translate-y-1/2 text-white space-y-4"
        >
          <h2 class="text-3xl lg:text-5xl font-bold drop-shadow-md">
            Higher Eduction Access Prediction System
          </h2>
          <p class="text-sm lg:text-lg max-w-md drop-shadow-md">
            This future university will help your way
          </p>
          <%if(locals.user){%>
          <a href="/university" class="btn btn-primary"> University List</a>
          <%}%>
        </div>
        <div
          class="absolute left-5 right-5 top-1/2 flex -translate-y-1/2 transform justify-between"
        >
          <a href="#slide3" class="btn btn-circle btn-outline btn btn-primary"
            >❮</a
          >
          <a href="#slide2" class="btn btn-circle btn-outline btn btn-primary"
            >❯</a
          >
        </div>
      </div>

      <!-- Repeat for other slides with different text -->
      <div id="slide2" class="carousel-item relative w-full">
        <img src="/images/slider-2.jpg" class="w-full object-cover" />
        <div
          class="absolute top-1/2 left-5 lg:left-16 -translate-y-1/2 text-white space-y-4"
        >
          <h2 class="text-3xl lg:text-5xl font-bold drop-shadow-md text-info">
            Question for your skill
          </h2>
          <p class="text-sm lg:text-lg max-w-md drop-shadow-md text-primary">
            Can check your skill with question
          </p>
          <%if(locals.user){%>
          <a href="/questions" class="btn btn-primary">Check Question</a>
          <%}%>
        </div>
        <div
          class="absolute left-5 right-5 top-1/2 flex -translate-y-1/2 transform justify-between"
        >
          <a href="#slide1" class="btn btn-circle btn-outline btn btn-primary"
            >❮</a
          >
          <a href="#slide3" class="btn btn-circle btn-outline btn btn-primary"
            >❯</a
          >
        </div>
      </div>
      <div id="slide3" class="carousel-item relative w-full">
        <img src="/images/slider-3.jpg" class="w-full object-cover" />
        <div
          class="absolute top-1/2 left-5 lg:left-16 -translate-y-1/2 text-white space-y-4"
        >
          <h2 class="text-3xl lg:text-5xl font-bold drop-shadow-md text-info">
            Result With AI Prediction
          </h2>
          <p class="text-sm lg:text-lg max-w-md drop-shadow-md text-primary">
            Final result will include which university can join
          </p>
          <%if(locals.user){%>
          <a href="/user" class="btn btn-primary">Explore User Dashboard</a>
          <%}%>
        </div>
        <div
          class="absolute left-5 right-5 top-1/2 flex -translate-y-1/2 transform justify-between"
        >
          <a href="#slide2" class="btn btn-circle btn-outline btn btn-primary"
            >❮</a
          >
          <a href="#slide1" class="btn btn-circle btn-outline btn btn-primary"
            >❯</a
          >
        </div>
      </div>
    </div>
    <div class="aos-all">
      <div class="swiper" data-aos="fade-up">
        <!-- Additional required wrapper -->
        <div class="text-center bg-base-200 text-3xl lg:text-5xl font-semibold">
          Top University
        </div>
        <div class="swiper-wrapper">
          <!-- Slides -->
          <div class="swiper-slide">
            <div class="hero bg-base-200 h-[50vh]">
              <div class="hero-content flex-col lg:flex-row">
                <img
                  src="https://edurank.org/assets/img/uni-logos/university-of-computer-studies-yangon-logo.jpg"
                  class="max-w-sm rounded-lg shadow-2xl"
                />
                <div>
                  <h1 class="text-xl lg:text-5xl font-bold">
                    University of Computer Studies Yangon
                  </h1>
                  <p class="py-6">
                    A leading institution for computer science and related
                    fields
                  </p>
                  <button class="btn btn-primary">See More</button>
                </div>
              </div>
            </div>
          </div>
          <div class="swiper-slide">
            <div class="hero bg-base-200 h-[50vh]">
              <div class="hero-content flex-col lg:flex-row">
                <img
                  src="https://edurank.org/assets/img/uni-logos/university-of-medicine-mandalay-logo.png"
                  class="max-w-sm rounded-lg shadow-2xl"
                />
                <div>
                  <h1 class="text-xl lg:text-5xl font-bold">
                    University of Medicine, Mandalay
                  </h1>
                  <p class="py-6">
                    A leading institution for medicine and related fields
                  </p>
                  <button class="btn btn-primary">Get Started</button>
                </div>
              </div>
            </div>
          </div>
          <div class="swiper-slide">
            <div class="hero bg-base-200 h-[50vh]">
              <div class="hero-content flex-col lg:flex-row">
                <img
                  src="data:image/png;base64,"
                  class="max-w-sm rounded-lg shadow-2xl"
                />
                <div>
                  <h1 class="text-xl lg:text-5xl font-bold">
                    Yangon Technological University
                  </h1>
                  <p class="py-6">
                    YTU is the country's oldest and largest engineering
                    university, and the best engineering university in Myanmar.
                  </p>
                  <button class="btn btn-primary">Get Started</button>
                </div>
              </div>
            </div>
          </div>
          ...
        </div>
        <!-- If we need pagination -->
        <!-- <div class="swiper-pagination"></div> -->

        <!-- If we need navigation buttons -->
        <!-- <div class="swiper-button-prev"></div>
      <div class="swiper-button-next"></div> -->

        <!-- If we need scrollbar -->
        <!-- <div class="swiper-scrollbar"></div> -->
      </div>
      <div class="swiper2 mt-2" data-aos="fade-up">
        <!-- Additional required wrapper -->
        <div
          class="text-center bg-blue-200 text-3xl lg:text-5xl font-thin italic"
        >
          User Feedback
        </div>
        <div class="swiper-wrapper">
          <!-- Slides -->
          <div class="swiper-slide">
            <div class="hero bg-blue-200 h-[50vh]">
              <blockquote
                class="p-6 border-l-4 border-primary bg-base-100 shadow-md rounded-lg"
              >
                <p class="text-lg italic text-gray-700">
                  "Education is the most powerful weapon which you can use to
                  change the world."
                </p>
                <footer class="mt-4 text-sm text-gray-500 text-right">
                  — Nelson Mandela
                </footer>
              </blockquote>
            </div>
          </div>
          <div class="swiper-slide">
            <div class="hero bg-blue-200 h-[50vh]">
              <blockquote
                class="p-6 border-l-4 border-primary bg-base-100 shadow-md rounded-lg"
              >
                <p class="text-lg italic text-gray-700">
                  "Education is the most powerful weapon which you can use to
                  change the world."
                </p>
                <footer class="mt-4 text-sm text-gray-500 text-right">
                  — Nelson Mandela
                </footer>
              </blockquote>
            </div>
          </div>
          <div class="swiper-slide">
            <div class="hero bg-blue-200 h-[50vh]">
              <blockquote
                class="p-6 border-l-4 border-primary bg-base-100 shadow-md rounded-lg"
              >
                <p class="text-lg italic text-gray-700">
                  "Education is the most powerful weapon which you can use to
                  change the world."
                </p>
                <footer class="mt-4 text-sm text-gray-500 text-right">
                  — Nelson Mandela
                </footer>
              </blockquote>
            </div>
          </div>
          ...
        </div>
        <!-- If we need pagination -->
        <div class="swiper-pagination2"></div>
      </div>
    </div>
    <% include ./partial/footer.ejs %>
  </body>
  <script src="/javascripts/aos.js"></script>
  <script>
    AOS.init({
      mirror: true,
    });
    const swiper = new Swiper(".swiper", {
      // Optional parameters
      direction: "horizontal",
      loop: true,
      pagination: {
        el: ".swiper-pagination",
      },
      autoplay: {
        delay: 5000,
      },
    });

    const swiper2 = new Swiper(".swiper2", {
      // Optional parameters
      direction: "horizontal",
      loop: true,
      pagination: {
        el: ".swiper-pagination",
      },
      autoplay: {
        delay: 3000,
      },
      scrollbar: {
        el: ".swiper-scrollbar",
      },
    });
  </script>
</html>
